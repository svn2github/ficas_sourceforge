#!/bin/sh

AXIOM=./axp

BBLD=@BBLD@

PROG=$BBLD/interpsys

CDIR=.

# Phase 1

echo Phase 1
# DAASE=/mnt/a10/pom/pp/axiom/axp4/ax-build1/

DAASE=$CDIR/r1
(export AXIOM DAASE; \
 echo ')read "boo1.input"' | $PROG)

# Phase 2

echo Phase 2

DAASE=$CDIR/r2

mkdir -p $DAASE/algebra
mv browse.daase  category.daase  compress.daase  interp.daase \
     operation.daase $DAASE/algebra || exit 1

for A in *.NRLIB; do
  B=${A%.NRLIB};
  cp $A/code.o ${AXIOM}/algebra/$B.o ;
done || exit 1

(export AXIOM DAASE; \
 echo ')read "boo2.input"' | $PROG)


